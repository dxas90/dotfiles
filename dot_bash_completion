type zoxide >/dev/null 2>&1 && source <(zoxide init $MY_SHELL)
type chezmoi >/dev/null 2>&1 && source <(chezmoi completion $MY_SHELL)
type kubectl >/dev/null 2>&1 && source <(kubectl completion $MY_SHELL)
type task >/dev/null 2>&1 && source <(task --completion $MY_SHELL)
# type flux >/dev/null 2>&1 && source <(flux completion $MY_SHELL)  # 13k lines
# type vcluster >/dev/null 2>&1 && source <(vcluster completion $MY_SHELL)

complete -F __start_kubectl k
complete -o default -o nospace -F __start_chezmoi cm

